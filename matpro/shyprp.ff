      subroutine shyprp(ssol,sliq)
c
c     shyprp returns 304 stainless steel phase transition temperatures
c
c     ssol   = output solidus temperature (k)
c     sliq   = output liquidus temperature (k)
c
c     shyprp was coded by d. l. hagrman march 1982
c
$if def,in32,1
       implicit real*8(a-h,o-z)
c
      ssol = 1671.
      sliq = 1727.
      return
      end
