      function asten(ictype)
!  Returns the interfacial surface tension of molten neutron absorbers
!  on zircaloy surfaces.
!     asten  = output interfacial surface tension (n/m)
!     ictype = input absorber material type
!                 1 = silver-indium-cadmium (.8,.15,.05)
!                 2 = boron carbide
!
!  Coded by d. l. hagrman march 1982.
!
       implicit none
       real asten
       integer ictype
!
       asten = 0.3
       return
       end
