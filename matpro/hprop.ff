      subroutine hprop(hsol,hliq)
c
c     hprop returns inconel 718 phase transition temperatures
c
c     hsol   = output solidus temperature (k)
c     hliq   = output liquidus temperature (k)
c
c     hprop was coded by d. l. hagrman march 1982
c
$if def,in32,1
       implicit real*8(a-h,o-z)
c
      hsol = 1533.
      hliq = 1609.
      return
      end
