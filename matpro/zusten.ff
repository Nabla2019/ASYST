      function zusten(zutemp)
c
c     zusten returns the interfacial surface tension of molten
c     zirconium-uranium-oxygen compounds on zircaloy rods
c
c     zusten = output interfacial surface tension (n/m)
c     zutemp = input compound temperature (k)
c
c     zusten was coded by d. l. hagrman march 1982
c
       implicit none
       real zusten,zutemp
c
       zusten = 0.45
       return
       end
