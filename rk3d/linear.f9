C                                                                      C
                 SUBROUTINE LINEAR(T1,T2,TIN,Y1,Y2,YOUT)
C                                                                      C
C23456789012345678901234567890123456789012345678901234567890123456789012
C
C
C  THIS SUBROUTINE COMPLETES LINEAR INTERPOLATION OR EXTRAPOLATION
C
C
C23456789012345678901234567890123456789012345678901234567890123456789012
C
      REAL*8 Y1,Y2,T1,T2,TIN,YOUT !vmq
 
      YOUT = Y1 + ( (Y2 - Y1 )/(T2 - T1) ) * (TIN - T1)
 
      RETURN
      END
