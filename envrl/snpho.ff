*deck snpho
        subroutine snpho(ludbg,subnam,string)
        implicit none
c
c   write out a string of characters
c
c   subroutine arguments
        integer         ludbg
        character*(*)   subnam
        character*(*)   string
c
        write(ludbg,800) subnam, string
c
 800    format(a,': ',a)
c
        return
        end
