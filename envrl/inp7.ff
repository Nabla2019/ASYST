       subroutine inp7 (card,item)
c
c  This subroutine prints an error message on the output file that
c  item on card is in error.
c
c  Cognizant engineer: rjw.
c
       implicit none
       integer card,item
       include 'efiless.h'
       include 'efilesd.h'
c
       write (output,"('0******** Item',i3,' on card',i10,' in error.')"
     & ) item,card
       return
       end
