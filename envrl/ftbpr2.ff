       subroutine ftbpr2 (id1,indx1,id2,indx2)
c
c  This subroutine is used to process the next two process files that
c  have been opened in read, write, or read-write access mode.
c
c  Cognizant engineer: rjw
c
*call ftbcom
       real id1, id2
       call ftbpr1 (id2,indx2)
       call ftbpr1 (id1,indx1)
       return
       end
