       subroutine ftbpr4 (id1,indx1,id2,indx2,id3,indx3,id4,indx4)
c
c  This subroutine is used to process the next four process files that
c  have been opened in read, write, or read-write access mode.
c
c  Cognizant engineer: rjw
c
       real id1, id2, id3, id4
       call ftbpr1 (id4,indx4)
       call ftbpr1 (id3,indx3)
       call ftbpr1 (id2,indx2)
       call ftbpr1 (id1,indx1)
       return
       end
