*mlist
*if def,selap
       function tarea(x1,x2,x3,y1,y2,y3)
c
c
c  Descriptive sentence for tarea goes here.
c
c  Cognizant engineer: ewc.
c
c This is a COUPLE ROUTINE
c
       implicit none
c
c  Local variables.
       real x1,x2,x3,y1,y2,y3
       real tarea
c
      tarea=(x2*y3-x2*y1+x1*y2-x1*y3+x3*y1-x3*y2)/2.
      tarea=abs(tarea)
      return
      end
*endif
